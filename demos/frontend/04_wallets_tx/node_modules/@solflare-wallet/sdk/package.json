    "babel-loader": "^8.2.3",
    "core-js": "^3.20.1",
    "css-loader": "^5.2.7",
    "html-webpack-plugin": "^4.5.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^2.0.0",
    "typescript": "^4.5.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1",
